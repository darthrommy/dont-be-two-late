		"react-router": "^7.11.0",
		"zod": "^4.2.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.10",
		"@cloudflare/vite-plugin": "^1.17.1",
		"@react-router/dev": "^7.11.0",
		"@react-router/remix-routes-option-adapter": "^7.11.0",
		"@tailwindcss/vite": "^4.1.18",
		"@types/node": "^25.0.3",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"consola": "^3.4.2",
		"dotenv": "^17.2.3",
		"remix-flat-routes": "^0.8.5",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"vite": "^7.3.0",
